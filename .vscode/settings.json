{
  "search.exclude": {
    "**/coverage": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "css.lint.validProperties": ["@layer", "@global", "@value"],
  "tailwindCSS.experimental.configFile": ".storybook/styles/styles.css",
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    ".*ClassName"
  ],
  "i18n-ally.pathMatcher": "{locale}.{ext}",
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.namespace": true,
  "i18n-ally.enabledFrameworks": ["react-i18next"],
  "i18n-ally.localeCountryMap": {
    "en": "GB",
    "ar": "AE"
  },
  "i18n-ally.localesPaths": ["packages/**/src/i18n"]
}
